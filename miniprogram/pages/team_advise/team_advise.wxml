<!--pages/team_advise/team_advise.wxml


显示建议
提建议按钮

-->
<van-tabbar active="{{ active }}" fixed="{{false}}" bind:change="directTo">
    <van-tabbar-item icon="search">
        <text>建议</text>
    </van-tabbar-item>
    <van-tabbar-item icon="friends-o">
            <text>信息</text>
    </van-tabbar-item>
    
  </van-tabbar>
<view>
        <van-icon name="comment-o" size="20px" bind:click="addAdvise"/>
    <text>提出意见</text>
</view>
当前团队{{currentTeamName}}

<view  wx:for="{{adviceList}}">
<van-collapse value="{{ activeName }}" bind:change="onChange" accordion>
  <van-collapse-item title="{{index+1}}" name="{{index}}">
    {{item.content}}
  </van-collapse-item>
</van-collapse>
</view>
  
