<!--pages/team_add_advise/team_add_advise.wxml

提建议页面：
写入建议、表单提交

-->


<van-cell-group>
  <van-field
    value="{{ message }}"
    label="提出我的建议"
    type="textarea"
    placeholder="我有一个大胆的想法"
    bind:change="changeInput"
    autosize
    border="{{ false }}"
  />
</van-cell-group>
<van-button slot="button" size="small"
bind:click="upBtn"
 type="primary">
 提交我的意见</van-button>